Algoritmo "senha_fixa"

Var

   senha, input : inteiro

Inicio

   senha <- 2002
   
   escreva("Digite a senha: ")
   leia(input)
   
   enquanto input <> senha faca
   escreva("Senha invalida! Tente novamente: ")
   leia(input)
   fimenquanto
   escreval("Acesso permitido")

Fimalgoritmo