Algoritmo "consumo"

Var

   distancia, combustivel, consumo : real

Inicio

      escreva("Digite a distancia percorrida: ")
      leia(distancia)
      escreva("Digite a quantidade de combustivel gasto: ")
      leia(combustivel)
      consumo<- distancia / combustivel
      escreva("Consumo medio = ", consumo:1:3)
      
   


Fimalgoritmo